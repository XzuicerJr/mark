model Habit {
  id          String   @id @default(uuid())
  name        String
  description String?
  icon        String
  color       String
  startDate   DateTime

  userId String
  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([userId])
}
